<core:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m"
		controllerName="emplist.Employees" xmlns:html="http://www.w3.org/1999/xhtml">
	<Page title="{i18n>title}">
		<content>
	       <Table id="idtable" items="{/element}">
	          <columns>
	             <Column>
	                <header>
	                   <Text text="{i18n>name}"/>
	                </header>
	             </Column>
	             <Column>
	                  <header>
	                   <Text text="{i18n>dept}"/>
	                </header>
	             </Column>
	             <Column>
	                 <Text text="{i18n>role}"/>
	             </Column>
	          </columns>
	          
	          <items>
	             <ColumnListItem>
	                <cells>
	                  <Text text="{name}"/>
	                  <Text text="{dept}"/>
	                  <Text text="{role}"/>
	                </cells>
	             </ColumnListItem>
	          </items>
	          
	       </Table>
		</content>
	</Page>
</core:View>